<launch>

  <!-- Pass in mocap_config_file:=/path/to/config.yaml to change options. -->
  <arg name="mocap_config_file"
       default="$(find mocap_optitrack)/config/two_cars.yaml" />

  <node pkg="mocap_optitrack"
        type="mocap_node"
        name="mocap_node"
        required="true">
  </node>

  <node type="rviz" name="rviz" pkg="rviz" args="-d $(find datmo)/config/live_experiment.rviz" />

  <node pkg="rosbag" type="record" name="recorder" output="screen" args="-O /home/kostas/experiments/exp.bag /scan /tf /ego_pose /red_pose" />
  
  <!--<include file="$(find localization)/launch/mocap_localization.launch"/>-->

